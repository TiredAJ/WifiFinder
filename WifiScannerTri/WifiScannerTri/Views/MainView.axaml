<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:WifiScannerTri.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WifiScannerTri.Views.MainView"
             x:DataType="vm:WifiInfoViewModel"
			 >

	<StackPanel>
		<DataGrid x:Name="DGRD_Wifis" Margin="10" AutoGenerateColumns="True" 
				  ItemsSource="{Binding WifiInfoItems}" IsReadOnly="True" GridLinesVisibility="All"
				  IsVisible="True">
		</DataGrid>
		<TextBlock Text="{Binding TempTest}" IsVisible="True"> hello</TextBlock>
		
	</StackPanel>
	
</UserControl>
